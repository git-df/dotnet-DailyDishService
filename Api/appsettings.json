{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CacheOptions": {
    "DefaultCacheDuration": 36000,
    "DefaultPriority": "NeverRemove"
  },
  "HangfireOptions": {
    "ProcessDailyDishCron": "* 8-10 * * *"
  }
}
